{
  "manifest_version": 2,
  "name": "boltlane",
  "description": "Personal offline password manager",
  "version": "0.2",
  "browser_action": {
  "default_icon": "popup/icon.png",
  "default_popup": "popup/popup.html"
},
  "permissions": [
      "tabs"
  ],
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["boltlane.js"]
  }
]
}